                <div class="title">Breaking News :</div>
                <div class="news-wrap">
                    <div class="row slick-marquee" itemscope itemtype="http://schema.org/ItemList">
                        {% for news in breaking %}
                            <div class="col-auto" itemprop="itemListElement" itemscope itemtype="http://schema.org/Article">
                                <a href="blog-details.html" class="breaking-news" itemprop="url">
                                    <span itemprop="headline">{{ news.title }}</span>
                                </a>
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </div>
